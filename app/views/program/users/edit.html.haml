- user = @object
%h2= user.fullname
.span-6
  %table
    -%w{email phone processed sign_in_count last_sign_in_at }.each do |field|
      %tr{:class => field}
        %td.bold= field.titleize
        %td= user.read_attribute(field)
    
.span-6
  = form_for user, :url => object_update_path("users", user) do |f|
    = f.label :processed
    = f.select :processed, options_for_select([["True", true], ["False", false]])
    = f.submit
    